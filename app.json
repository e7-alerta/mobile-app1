{
  "expo": {
    "name": "Alarma PBA",
    "slug": "alerta-pba",
    "version": "1.2.22",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "e7.vecinos.alertapba",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "owner": "e7lab",
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "e7.vecinos.ave"
    },
    "android": {
      "googleServicesFile": "./google-services.json",
      "versionCode": 22,
      "package": "e7.vecinos.alertapba",
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": [
        "android.permission.ACCESS_COARSE_LOCATION",
        "android.permission.ACCESS_FINE_LOCATION",
        "android.permission.FOREGROUND_SERVICE"
      ],
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyBX-H1Lk9ubEY-tLvzgel77UFY2fIf5yzM"
        }
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      ["expo-location",
      {
        "locationAlwaysAndWhenInUsePermission": "Permitanos comprobar tu ubicacion."
      }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "38dcf7e5-6d1f-4f54-a973-99f8e8cf2fba"
      }
    }
  }
}
